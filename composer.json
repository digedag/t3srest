{
	"name" : "digedag/t3srest",
	"description" : "Extends t3rest for t3sports classes.",
	"type" : "typo3-cms-extension",
	"keywords" : [
		"TYPO3 CMS",
		"t3sports"
	],
	"homepage" : "http://www.system25.de/",
	"license" : "GPL-2.0-or-later",
	"authors" : [{
			"name" : "Ren√© Nitzsche",
			"email" : "rene@system25.de",
			"role" : "Developer"
		}
	],
	"support" : {
		"email" : "rene@system25.de",
		"source" : "https://github.com/digedag/t3srest",
		"issues" : "https://github.com/digedag/t3srest/issues"
	},
	"replace" : {
		"typo3-ter/t3srest" : "self.version"
	},
	"require" : {
		"typo3/cms-core": "^10.4 || ^11.5 || ^12.4 || ^13.4",
		"digedag/rn-base" : "^1.19"
	},
	"autoload" : {
		"psr-4": {
			"System25\\T3srest\\": "Classes"
		}
	},
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "extension-key": "t3srest",
            "web-dir": ".Build/Web"
      }
    }
}
